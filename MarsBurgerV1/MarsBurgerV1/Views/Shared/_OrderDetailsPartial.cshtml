@model MarsBurgerV1.Models.OrderDetailsPartial
<td>
    @if (Model.ImageUrl != null)
    {
        <img src="@Url.Content(Model.ImageUrl)" alt="IMAGES" height="100" width="100" />
    }
</td>
<td>
    @Html.DisplayFor(m=>m.Name)
</td>
<td>
    @Html.DisplayFor(m => m.Quantity)
</td>
<td>
    @Html.DisplayFor(m => m.Price)
</td>