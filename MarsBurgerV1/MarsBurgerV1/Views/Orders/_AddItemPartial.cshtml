@model IEnumerable<MarsBurgerV1.Models.OrderItem>
@using MarsBurgerV1.Models
@{
    Layout = null;
}

@foreach (var i in Model)
{
    <div class="orderItem">
        <div class="form-group">
            @Html.Label("Meals", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                
                @Html.ListBox(model => model.LastUpdate, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.LastUpdate, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
    <hr />
}
